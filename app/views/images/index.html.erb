<!-- #this is what gets rendered at /index -->
<h1 class="title">Most Recent Images</h1>

  <section class="image-container">


      <% @most_recent_images.each do |image| %>
      <div class="img">
        <%= link_to image_tag(image.url), image_path(image) %>
        <p class="image-details"><%= image.title %>, <%= image.artist %></p>
        </div>
      <% end %>



</section>

<% @images_1_month.each do |image| %>
<div class="img">
  <%= link_to image_tag(image.url), image_path(image) %>
  <p class="image-details"><%= image.title %>, <%= image.artist %></p>
  </div>
<% end %>
