<!-- #this is what gets rendered at /index -->
<h1 class="title">Images</h1>

  <section class="image-container">


      <% @images.each do |image| %>
      <div class="img">
        <%= link_to image_tag(image.url), "/images/#{image.id}" %>
        <p class="image-details"><%= image.title %>, <%= image.artist %></p>
        </div>
      <% end %>

</section>
