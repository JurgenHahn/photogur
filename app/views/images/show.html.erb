<section class="home-button-2"><%= link_to "Home", images_url %></section>
<section class="edit"><%= link_to "Edit", edit_image_path(@image) %></section>
<section class="delete"><%= link_to "Delete", @image, method: :delete, data: {confirm: "Are you sure you want to delete this picture?"} %></section>

<section class="ind-image-wrap">
<section class="ind-image">
<%= image_tag @image.url %>
<p><%= @image.title %>, <%= @image.artist %></p>
</section>
</section>
